{
  "name": "studio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sanity dev",
    "format": "prettier --write .",
    "start": "sanity start",
    "lint": "eslint .",
    "build": "sanity build && sanity schema deploy --manifest-dir ./dist/static",
    "deploy": "sanity deploy",
    "type": "sanity schema extract --enforce-required-fields && sanity typegen generate",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@portabletext/react": "^3.2.3",
    "@sanity/asset-utils": "^2.2.1",
    "@sanity/assist": "^5.0.0",
    "@sanity/dashboard": "^4.1.4",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^1.1.0",
    "@sanity/table": "^1.1.4",
    "@sanity/ui": "^3.0.7",
    "@sanity/vision": "^4.5.0",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sanity": "4.5.0",
    "sanity-plugin-documents-pane": "^2.4.1",
    "sanity-plugin-media": "^4.0.0",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/react": "^19.1.10",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "typescript-eslint": "^8.40.0"
  }
}
