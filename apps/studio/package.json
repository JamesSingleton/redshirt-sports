{
  "name": "@redshirt-sports/studio",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "sanity dev",
    "format": "prettier --write .",
    "start": "sanity start",
    "lint": "eslint .",
    "build": "sanity build && sanity schema deploy --manifest-dir ./dist/static",
    "deploy": "sanity deploy",
    "type": "sanity schema extract --enforce-required-fields && sanity typegen generate",
    "check:types": "tsc --noEmit"
  },
  "dependencies": {
    "@portabletext/react": "^6.0.0",
    "@sanity/asset-utils": "^2.3.0",
    "@sanity/assist": "^5.0.3",
    "@sanity/blueprints": "^0.7.1",
    "@sanity/client": "^7.13.2",
    "@sanity/dashboard": "^5.0.1",
    "@sanity/functions": "^1.1.0",
    "@sanity/icons": "^3.7.4",
    "@sanity/image-url": "^2.0.2",
    "@sanity/table": "^2.0.0",
    "@sanity/ui": "^3.1.11",
    "@sanity/vision": "^5.1.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sanity": "5.1.0",
    "sanity-plugin-documents-pane": "^3.0.1",
    "sanity-plugin-media": "^4.1.1",
    "slugify": "^1.6.6",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "typescript-eslint": "^8.50.1"
  }
}
