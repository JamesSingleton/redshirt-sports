{
  "name": "redshirt-sports",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "postbuild": "if [ -f './.next/serverless/lib/build-rss.js' ]; then node ./.next/serverless/lib/build-rss.js; else node ./.next/server/lib/build-rss.js; fi"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.4",
    "@heroicons/react": "^1.0.6",
    "@portabletext/react": "^1.0.6",
    "@portabletext/to-html": "^1.0.3",
    "@sanity/asset-utils": "^1.2.3",
    "@sanity/image-url": "^1.0.1",
    "@sanity/webhook": "^1.0.2",
    "@sentry/nextjs": "^7.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "classnames": "^2.3.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.28.0",
    "feed": "^4.2.2",
    "htm": "^3.1.1",
    "next": "^12.1.6",
    "next-plausible": "^3.2.0",
    "next-sanity": "^0.5.2",
    "next-seo": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-share": "^4.4.0",
    "rss-parser": "^3.12.0",
    "sharp": "^0.30.6",
    "sitemap": "^7.1.1",
    "vhtml": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "autoprefixer": "^10.4.7",
    "critters": "^0.0.16",
    "eslint": "^8.17.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.14",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^10.1.8",
    "postcss-preset-env": "^7.7.1",
    "prettier": "^2.7.0",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.7.3"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [],
    "entrypoints": [
      "pages"
    ]
  }
}
