{
  "name": "redshirt-sports",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier-fix": "prettier --write .",
    "find:unused": "npx next-unused",
    "postbuild": "next-sitemap && if [ -f './.next/serverless/lib/build-rss.js' ]; then node ./.next/serverless/lib/build-rss.js; else node ./.next/server/lib/build-rss.js; fi",
    "analyze": "ANALYZE=true next build"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.1",
    "@heroicons/react": "^2.0.11",
    "@next/bundle-analyzer": "^12.3.0",
    "@portabletext/react": "^1.0.6",
    "@portabletext/to-html": "^1.0.3",
    "@sanity/asset-utils": "^1.3.0",
    "@sanity/image-url": "^1.0.1",
    "@sanity/webhook": "^2.0.0",
    "@sentry/nextjs": "^7.12.1",
    "@sentry/tracing": "^7.12.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "feed": "^4.2.2",
    "htm": "^3.1.1",
    "next": "^12.3.0",
    "next-plausible": "^3.6.2",
    "next-sanity": "^0.8.4",
    "next-sitemap": "^3.1.22",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sharp": "^0.31.0",
    "vhtml": "^2.2.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.7.17",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.10",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.16",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nesting": "^10.1.10",
    "postcss-preset-env": "^7.8.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3"
  },
  "next-unused": {
    "alias": {
      "@lib/*": [
        "lib/*"
      ],
      "@styles/*": [
        "styles/*"
      ],
      "@config/*": [
        "config/*"
      ],
      "@components/*": [
        "components/*"
      ]
    },
    "debug": true,
    "include": [
      "components",
      "lib",
      "pages"
    ],
    "exclude": [
      "lib/build-rss.js"
    ],
    "entrypoints": [
      "pages"
    ]
  }
}
